{
  "name": "gulp-minify-css",
  "description": "Minify css with clean-css.",
  "version": "1.0.0",
  "repository": "jonathanepollack/gulp-minify-css",
  "author": "Jonathan Pollack <jonathan@gnobel.com> (gnobel.com)",
  "contributors": [
    "Shinnosuke Watanabe <snnskwtnb@gmail.com> (github.com/shinnn)"
  ],
  "keywords": [
    "gulpplugin",
    "minify",
    "optimize",
    "optimise",
    "compress",
    "css",
    "clean-css"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jonathanepollack/gulp-minify-css/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "pretest": "jscs *.js test/*.js && eslint *.js test/*.js",
    "test": "mocha",
    "test-dot": "mocha --reporter dot",
    "coverage": "istanbul cover _mocha",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "dependencies": {
    "clean-css": "3.1.9",
    "gulp-util": "^3.0.4",
    "object-assign": "^2.0.0",
    "through2": "^0.6.3",
    "vinyl-bufferstream": "^1.0.1",
    "vinyl-sourcemaps-apply": "^0.1.4"
  },
  "devDependencies": {
    "chai": "^2.1.0",
    "eslint": "^0.19.0",
    "gulp": "^3.8.8",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-stylus": "^2.0.0",
    "istanbul": "^0.3.6",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.11.3",
    "mocha": "^2.2.1",
    "simple-bufferstream": "1.0.0",
    "stream-combiner2": "^1.0.2",
    "vinyl": "^0.4.6"
  },
  "jscsConfig": {
    "preset": "google",
    "disallowMultipleVarDecl": null,
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
